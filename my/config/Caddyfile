# tls /path/to/your/certificate.crt /path/to/your/certificate.key

# 代理 Xray
reverse_proxy localhost:3002 {
  header_up Host vless
  path /${VLESS_WSPATH}
}
reverse_proxy localhost:3003 {
  header_up Host vmess
  path /${VMESS_WSPATH}
}

# 代理其他服务
reverse_proxy localhost:${V_PORT}

# 伪装静态主页
handle_path / {
  static /app
}
